{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fdaprojectfactory"
		},
		"FDA_DL_STORAGE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'FDA_DL_STORAGE'"
		},
		"FDA_DL_STORAGE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fdaprojectlake.dfs.core.windows.net/"
		},
		"FDA_HTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://download.open.fda.gov"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fdaprojectfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FDA_DL_STORAGE_accountKey": {
						"value": "[parameters('FDA_DL_STORAGE_accountKey')]"
					},
					"FDA_DL_STORAGE_properties_typeProperties_url": {
						"value": "[parameters('FDA_DL_STORAGE_properties_typeProperties_url')]"
					},
					"FDA_HTTP_properties_typeProperties_url": {
						"value": "[parameters('FDA_HTTP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}